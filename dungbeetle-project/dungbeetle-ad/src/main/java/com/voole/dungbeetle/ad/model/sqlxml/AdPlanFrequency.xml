<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
	
<mapper namespace="adPlanFrequency">

	<update id="updateAdPuttimes" parameterType="hashmap">
		UPDATE voole_ad.ad_plan SET playtimes = playtimes + #{puttimes} WHERE seqno=#{seqno}
	</update>
 </mapper>